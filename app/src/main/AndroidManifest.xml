<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ruchita.wifi_final_project">

    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/wifi_student"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TeacherActivity" />
        <activity android:name=".Admin" />
        <activity android:name=".Add_Student" />
        <activity android:name=".Update_Student" />
        <activity android:name=".Add_Teacher" />
        <activity android:name=".Update_Teacher" />
        <activity android:name=".SMSActivity" />
        <activity android:name=".AccessPointActivity" />
        <activity android:name=".StudentActivity" />
        <activity android:name=".ViewDefaulters" />
        <activity android:name=".UnknownActivity"></activity>
    </application>

</manifest>